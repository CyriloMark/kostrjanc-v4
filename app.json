{
  "expo": {
    "name": "kostrjanc",
    "slug": "kostrjanc",
    "owner": "kostrjanc",
    "version": "0.6.0b",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#060A0D"
    },
    "notification": {
      "icon": "./assets/icons/icon.png",
      "color": "#060A0D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kostrjanc"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#060A0D"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "de.kostrjanc"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/CyriloMark/kostrjanc",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/icon.png",
          "color": "#5884B0"
        }
      ]
    ]
  }
}